<Window x:Class="Brenn_und_Plasmaschneidanlage.RenameSubprogram"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Brenn_und_Plasmaschneidanlage"
        mc:Ignorable="d" WindowStyle="ToolWindow"
        x:Name="windowRenameSub"
        Title="Unterprogramm umbenenen" Height="172.333" Width="454" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="Icons/path4716-0.png">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" FlowDirection="RightToLeft" Margin="0,10,0,5">
            <Button x:Name="btnCancel" Margin="10,0,5,0" Width="80" Click="btnCancel_Click" TabIndex="8">Abbruch</Button>
            <Button x:Name="btnOK" Margin="5,0,3,0" Width="80" TabIndex="7" Click="btnOK_Click" IsEnabled="False">OK</Button>
        </StackPanel>
        <StackPanel  Margin="5,10,10,0" DockPanel.Dock="Top">
            <Label>Alter Name:</Label>
            <TextBox x:Name="txtOld" IsReadOnly="True" Margin="5,0,0,0"/>
            <Label>Neuer Name:</Label>
            <TextBox x:Name="txtNew" Margin="5,0,0,0" TextChanged="btnNew_TextChanged"/>
        </StackPanel>
    </DockPanel>
</Window>
